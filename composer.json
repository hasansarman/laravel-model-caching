{
    "name": "genealabs/laravel-model-caching",
    "description": "Automatic caching for Eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "genealabs/laravel-pivot-events": "*",
        "illuminate/cache": "*",
        "illuminate/config": "*",
        "illuminate/console": "*",
        "illuminate/database": "*",
        "illuminate/support": "*",
        "php": ">7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "*",
        "mockery/mockery": "*",
        "orchestra/database": "3.8.*",
        "orchestra/testbench-browser-kit": "3.8.*",
        "orchestra/testbench": "3.8.*",
        "php-coveralls/php-coveralls" : "*",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "^7.5",
        "predis/predis": "*",
        "sebastian/phpcpd": "*",
        "symfony/thanks": "*"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/database/factories",
            "tests/database/seeds"
        ],
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelModelCaching\\Providers\\Service"
            ]
        }
    }
}
